{
	"name": "phiz-app",
	"version": "0.2.0",
	"type": "module",
	"description": "Phiz is a tool that allows you to perform facial motion capture from any device and location.",
	"author": "SpookyCorgi",
	"repository": {
		"type": "git",
		"url": "https://github.com/SpookyCorgi/phiz.git"
	},
	"license": "GPL-3.0",
	"main": "src/electron.cjs",
	"scripts": {
		"dev": "cross-env NODE_ENV=dev npm run dev:all",
		"dev:all": "concurrently -n=svelte,electron -c='#ff3e00',blue \"npm run dev:svelte\" \"npm run dev:electron\"",
		"dev:svelte": "vite dev",
		"dev:electron": "electron src/electron.cjs",
		"build": "cross-env NODE_ENV=production npm run build:svelte",
		"build:svelte": "vite build",
		"build:electron": "electron-builder -mwl --config build.config.json"
	},
	"devDependencies": {
		"@skeletonlabs/skeleton": "^0.99.5",
		"@sveltejs/adapter-static": "1.0.5",
		"@sveltejs/kit": "1.2.10",
		"@tailwindcss/forms": "^0.5.3",
		"@types/nanoid-dictionary": "^4.2.0",
		"@typescript-eslint/eslint-plugin": "^5.49.0",
		"@typescript-eslint/parser": "^5.49.0",
		"autoprefixer": "^10.4.13",
		"concurrently": "^7.6.0",
		"cross-env": "^7.0.3",
		"dotenv": "^16.0.3",
		"electron": "^22.1.0",
		"electron-builder": "^23.6.0",
		"electron-connect": "^0.6.3",
		"electron-packager": "^17.1.1",
		"electron-updater": "^5.3.0",
		"eslint": "^8.32.0",
		"eslint-config-prettier": "^8.6.0",
		"eslint-plugin-svelte3": "^4.0.0",
		"npm-run-all": "^4.1.5",
		"postcss": "^8.4.21",
		"postcss-load-config": "^4.0.1",
		"prettier": "^2.8.3",
		"prettier-plugin-svelte": "^2.9.0",
		"sass": "^1.57.1",
		"svelte": "^3.55.1",
		"svelte-check": "^3.0.3",
		"svelte-preprocess": "^5.0.1",
		"tailwindcss": "^3.2.4",
		"tslib": "^2.4.1",
		"typescript": "^4.9.4",
		"vite": "^4.0.4"
	},
	"dependencies": {
		"@fontsource/rubik": "^4.5.14",
		"electron-context-menu": "^3.6.1",
		"electron-reloader": "^1.2.3",
		"electron-serve": "^1.1.0",
		"electron-window-state": "^5.0.3",
		"nanoid": "^4.0.0",
		"nanoid-dictionary": "^5.0.0-beta.1",
		"osc-js": "^2.4.0",
		"peerjs": "^1.4.7"
	}
}